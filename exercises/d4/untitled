people = [
  {
    :id => 1,
    :name => "bru"
  },
  {
    :id => 2,
    :name => "ski"
  },
  {
    :id => 3,
    :name => "brunette"
  },
  {
    :id => 4,
    :name => "ski"
  }
]

def find_by_name array, name 
	array.each do |item|
		if item[:name] == name.strip
			return item
		end
		return nil
	end
end

puts find_by_name(people, "ski")

puts find_by_name(people, "kitten!")


